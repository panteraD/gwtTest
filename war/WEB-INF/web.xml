<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <!-- no need in that since we are not using gwt rpc-->
    <servlet>
        <servlet-name>com.mySampleApplication.MySampleApplication MySampleApplicationService</servlet-name>
        <servlet-class>main.java.com.chernenkov.gwttest.server.MySampleApplicationServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.mySampleApplication.MySampleApplication MySampleApplicationService</servlet-name>
        <url-pattern>/MySampleApplication/MySampleApplicationService</url-pattern>
    </servlet-mapping>
</web-app>
